<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CloudAssignmentAC4.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CloudAssignmentAC4"
    Shell.FlyoutBehavior="Disabled">
    <FlyoutItem>
        <ShellContent
        Title="Image"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="File"
        ContentTemplate="{DataTemplate local:ImageUploadPage}"
        Route="MainPage" />
    </FlyoutItem>
    
</Shell>

